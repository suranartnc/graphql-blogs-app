{
  "name": "graphql-blogs-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel start-ssr-dev start-wds",
    "postinstall": "npm run build",
    "build": "npm run build-vendor && npm run build-client && npm run build-server",
    "build-vendor": "webpack --env=vendor -p --config ./.noxt/webpack/webpack.config.js",
    "build-client": "better-npm-run build-client",
    "build-server": "better-npm-run build-server",
    "start": "node ./server.bundle.js",
    "start-ssr-dev": "better-npm-run start-ssr-dev",
    "start-ssr-prod": "better-npm-run start-ssr-prod",
    "start-wds": "node ./.noxt/server/dev-server",
    "start-ssr-prod-bundle": "node ./server.bundle.js",
    "lint": "eslint --ext .js src"
  },
  "betterScripts": {
    "start-ssr-dev": {
      "command": "nodemon ./.noxt/server/ssr-server.js --exec babel-node",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "server",
        "BABEL_DISABLE_CACHE": 1
      }
    },
    "start-ssr-prod": {
      "command": "babel-node ./.noxt/server/ssr-server.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "server",
        "BABEL_DISABLE_CACHE": 1
      }
    },
    "build-server": {
      "command": "webpack --env=server --config ./.noxt/webpack/webpack.config.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "build-client": {
      "command": "webpack --env=prod --config ./.noxt/webpack/webpack.config.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suranartnc/graphql-blogs-app.git"
  },
  "author": "Suranart Niamcome",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suranartnc/graphql-blogs-app/issues"
  },
  "homepage": "https://github.com/suranartnc/graphql-blogs-app#readme",
  "dependencies": {
    "apollo-client": "^0.7.3",
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "^6.6.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-react-css-modules": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "graphql-tag": "^1.2.4",
    "http-proxy-middleware": "^0.17.3",
    "image-webpack-loader": "^3.2.0",
    "immutability-helper": "^2.1.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.1.1",
    "npm-run-all": "^3.1.2",
    "postcss-loader": "^1.2.1",
    "progress-bar-webpack-plugin": "^1.9.1",
    "react": "^15.4.1",
    "react-apollo": "^0.8.1",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.3.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-router-scroll": "^0.4.1",
    "redux": "^3.6.0",
    "sass-loader": "^4.1.1",
    "serve-favicon": "^2.3.2",
    "source-map-support": "^0.4.11",
    "style-loader": "^0.13.1",
