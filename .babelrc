{
  "presets": [
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "core": "./.core/",
        "components": "./src/app/components",
        "hocs": "./src/app/hocs",
        "modules": "./src/app/modules",
        "styles": "./src/app/styles",
        "utils": "./src/app/utils",
        "static": "./static"
      }
    }]
  ]
}
