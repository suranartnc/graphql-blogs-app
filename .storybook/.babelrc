{
  "presets": [
    "react",
    "es2015",
    "stage-1"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "core": "./.core/",
        "components": "./src/app/components",
        "hocs": "./src/app/hocs",
        "images": "./src/app/images",
        "modules": "./src/app/modules",
        "styles": "./src/app/styles",
        "utils": "./src/app/utils"
      }
    }]
  ]
}
