{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "core/*": [".core/*"],
      "components/*": ["src/app/components/*"],
      "hocs/*": ["src/app/hocs/*"],
      "images/*": ["src/app/images/*"],
      "modules/*": ["src/app/modules/*"],
      "styles/*": ["src/app/styles/*"],
      "utils/*": ["src/app/utils/*"]
    }
  }
}
